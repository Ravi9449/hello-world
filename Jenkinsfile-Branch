pipeline {
    agent any 
    stages{
        stage('Check'){
            steps{
                script {
                    //TAG_PREFIX = env.BRANCH_NAME | grep -oE 'PAP-[0-9]{4}'
                    env.TAG_PREFIX = env.BRANCH_NAME  
                    def TAG = sh(returnStdout: true, script: echo "${TAG_PREFIX}" | grep -oE "PAP-[0-9]{4}")
                    echo "${TAG}"
                }
            }
        }
    }
}