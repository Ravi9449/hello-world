pipeline {
    agent any
    
    parameters {
        string(name: 'SHELL_COMMAND_RESULT', description: 'Result of a shell command', defaultValue: getShellCommandOutput())
    }

    stages {
        stage('Do Something with the Shell Command Result') {
            steps {
                script {
                    def shellCommandResult = params.SHELL_COMMAND_RESULT
                    echo "Shell Command Result: ${shellCommandResult}"
                }
            }
        }
    }
}

def getShellCommandOutput() {
    return sh(script: 'echo "Hello, Shell Command Output"', returnStdout: true).trim()
}
