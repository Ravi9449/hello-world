#!groovy

pipeline {
   agent any
   parameters {
        activeChoiceParam('Namespace') {
            description('Select the Namespace')
            filterable(true)
            choiceType('PT_SINGLE_SELECT')
            groovyScript("""
                def environments = ['test', 'integration']
                return environments
            """)
            fallbackValue('Development')
        }
    }
   stages {
      stage('Stage1'){
         steps{
            script{
               echo "${currentBuild.projectName}"
            }
         }
      }

   } 
}

 